<h3><%=link_to 'Query Reviews', review_query_feed_entry_url(@feed_entry)%></h3>

<% unless @adj.nil? || @adj == 0%>
<table>
    <thead>
        <tr>
            <th>Word</th>
            <th>Count</th>

        </tr>
    </thead>
    <tbody>

        <%@adj = @adj.sort_by  {|k, v| -v}%>
        <%@adj.each do |k, v|%>
            <tr>
                    <td><%=k%></td>
                    <td><%=v%></td>

                </tr>
        <%end%>
    <%end%>
    </tbody>
</table>